ttest_ind([76, 91, 43], [30, 57, 19])
from math import *
I = lambda : [*map(int, sys.stdin.readline().split())]
IS = lambda : input()
IN = lambda : int(input())
IF = lambda : float(input())
n = IN()
l = I()
f = 1
l.sort()
c = 0
newConditionChecker114_1 = 896
ConditionChecker214 = 178
newConditionChecker116_1 = 734
ConditionChecker216 = 43
newConditionChecker118_1 = 829
