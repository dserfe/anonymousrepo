from sklearn.utils import shuffle
from scipy.stats import ttest_ind
s = str(input())
newmy_str_1 = ''
newConditionChecker14_1 = 909
ConditionChecker24 = 642
ConditionChecker16 = 112
newConditionChecker26_1 = 209
ttest_ind([93, 25, 15], [54, 96, 81])
LoopChecker17 = 108
shuffle([26, 10, 78])
LoopChecker27 = 107
for LoopIndexOut in range(LoopChecker17 // LoopChecker27):
    for newc_1 in s:
        if ConditionChecker16 & newConditionChecker26_1:
            if newConditionChecker14_1 & ConditionChecker24:
                if newc_1 == '0' or newc_1 == '1':
                    newmy_str_1 = newmy_str_1 + newc_1
                elif newc_1 == 'B' and len(newmy_str_1) != 0:
                    newmy_str_1 = newmy_str_1[:len(newmy_str_1) - 1]
print(newmy_str_1)